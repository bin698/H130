{
    "name": "JIASTON",
    "url": "sou.jiaston.com",
    "version": 100,
    "search": {
        "url": "https://sou.jiaston.com/search.aspx?key={key}&page=1&siteid=app2",
        "charset": "UTF-8",
        "list": "$.data[:5]",
        "name": "Name",
        "author": "Author",
        "cover": "Img",
        "detail": "$.Id"
    },
    "detail": {
        "catalog": ""
    },
    "catalog": {
        "list": "$..chapters[*]",
        "name": "$2",
        "chapter": "$1.html"
    },
    "chapter": {
        "content": "$.data.content"
    }
}