{
    "name": "App-4",
    "url": "shuapi.jiaston.com",
    "version": 101,
    "search": {
        "url": "https://shuapi.jiaston.com/search.aspx?key={key}",
        "charset": "UTF-8",
        "list": "$..data[*]",
        "name": "$.Name",
        "author": "$.Author",
        "cover": "$.Img",
        "detail": "https://shuapi.jiaston.com/book/{$.Id}/"
    },
    "detail": {
        "catalog": ""
    },
    "catalog": {
        "list": "$..list[*].list[*]",
        "name": "$.name",
        "chapter": "https://shuapi.jiaston.com/book/@get:{a}/{{$.id}}.html"
    },
    "chapter": {
        "content": "$..content"
    }
}